{
  "name": "mehr-als-nix/custom-phing-task-example",
  "description": "Example for integrating a custom task to phing by auto discovering.",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "phing-extension",
  "authors": [
    {
      "name": "Siad Ardroumli",
      "email": "siad.ardroumli@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrAlsNix\\CustomPhingTask\\": "src/"
    }
  },
  "extra": {
    "phing-custom-taskdefs": {
      "exampletask": "MehrAlsNix\\CustomPhingTask\\ExampleTask"
    },
    "phing-custom-typedefs": {
      "exampletype": "MehrAlsNix\\CustomPhingTask\\ExampleType"
    }
  }
}